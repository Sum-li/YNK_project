@import '../../common/common.wxss';
@import '../../common/iconfont.wxss';


page{
    position: relative;
    background-color: #fefefe;
    padding-bottom: 150rpx;
}
.block{
    background-color: #ededed;
    position: absolute;
    top: 0px;
    height: 300rpx;
    width: 100%;
    z-index: -2;
}


.search{
    position: fixed;
    top:30rpx;
    left: 30rpx;
    background-color: rgba(255,255,255,0.8);
    border-radius: 4rpx;
    z-index: 4;
    padding: 15rpx;
    width: 88%;
    margin: 0 auto
}




.main{
    position: absolute;
    top: 150rpx;
}
.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}


.touch{
    transform: translateY(5rpx);
    transition: all 0.05s;

}
.list{
}
.list>view{
    width: 45%;
    float: left;
    margin-left: 26rpx;
}
.good_box{
    margin-top: 30rpx;
    border-radius: 4rpx;
    overflow: hidden;
    background-color: #ffffff;
    box-shadow: 1px 0px 20rpx 6rpx rgba(0, 0, 0, 0.1) ;
}
.good-gphoto{
    width: 100%;
}
.good-name{
    font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
    font-size: 10pt;
    padding: 15rpx;
   color: gray;
}

